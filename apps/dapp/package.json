{
  "name": "dapp",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "postinstall": "prisma generate && prisma db push",
    "prisma:generate": "prisma generate",
    "prisma:migrate": "prisma migrate dev",
    "prisma:deploy": "prisma db push",
    "prisma:seed": "ts-node prisma/seed.ts",
    "db:reset": "psql $DATABASE_URL -c \"SELECT pg_terminate_backend(pid) FROM pg_stat_activity WHERE state = 'idle' AND pid <> pg_backend_pid();\" && prisma generate && prisma db push",
    "db:dump": "pg_dump $DATABASE_URL --no-owner --no-acl > database_dump.sql",
    "db:transfer": "pg_dump $DATABASE_URL --no-owner --no-acl | psql $STORAGE_DATABASE_URL"
  },
  "dependencies": {
    "@lottiefiles/dotlottie-react": "^0.13.2",
    "@mini/shared": "workspace:*",
    "@neondatabase/serverless": "^1.0.0",
    "@prisma/client": "5.22.0",
    "@safe-global/protocol-kit": "^6.0.2",
    "@safe-global/safe-core-sdk": "^3.3.5",
    "@safe-global/safe-core-sdk-utils": "^1.7.4",
    "@tanstack/react-query": "^5.71.1",
    "@worldcoin/minikit-js": "1.3.0",
    "@worldcoin/minikit-react": "^1.8.0",
    "alchemy-sdk": "^3.1.2",
    "axios": "^1.8.4",
    "date-fns": "^4.1.0",
    "eruda": "^3.2.3",
    "ethers": "^5.8.0",
    "framer-motion": "^12.6.2",
    "i18next": "^24.2.3",
    "i18next-browser-languagedetector": "^8.0.4",
    "lucide-react": "^0.484.0",
    "next": "15.2.4",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-i18next": "^15.4.1",
    "react-icons": "^5.5.0",
    "viem": "^2.24.3",
    "wagmi": "^2.14.16"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@svgr/webpack": "^8.1.0",
    "@tailwindcss/postcss": "^4",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "eslint": "^9",
    "eslint-config-next": "15.2.4",
    "prisma": "5.22.0",
    "tailwindcss": "^4",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.2"
  }
}
